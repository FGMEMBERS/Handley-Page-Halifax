<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-30.0</chase-distance-m>

  <view n="100">
    <name>Interior View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y"> -0.378 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y"> -0.058 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -6.828 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> 0 -0.058</pitch-offset-deg>
      <default-field-of-view-deg type="double"> 55 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Interior2 View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.441 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  0.755 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -5.973 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 55 </default-field-of-view-deg>
    </config>
  </view>

  <view n="102">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y"> 0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 2.760 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 8.281 </z-offset-m> <!--Back-->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 55 </default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y"> -0.352 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  1.350 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -6.727 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg> 60 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
